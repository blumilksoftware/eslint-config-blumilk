{
  "name": "@blumilksoftware/eslint-config-blumilk",
  "version": "1.0.0",
  "description": "Blumilk ESLint Config",
  "license": "MIT",
  "keywords": [
    "typescript",
    "eslint",
    "eslint-config",
    "lint",
    "config"
